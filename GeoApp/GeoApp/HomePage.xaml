<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" Title="GeoAware"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core"
            android:TabbedPage.ToolbarPlacement="Bottom"
            android:TabbedPage.BarBackgroundColor="White"
            android:TabbedPage.BarSelectedItemColor="Black"
            android:TabbedPage.BarTextColor="Black"
            xmlns:local="clr-namespace:GeoApp;assembly=GeoApp"
            x:Class="GeoApp.HomePage">
    <local:DataEntryListView Icon="data_icon"/>
    <local:MapView Icon="map_icon"/>
    <local:ImportView Icon="import_icon"/>
    <local:ExportView Icon="export_icon"/>
</TabbedPage>